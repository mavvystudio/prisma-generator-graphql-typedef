{
  "name": "prisma-generator-graphql-typedef",
  "description": "Generates a gql schema from prisma object",
  "version": "1.3.0",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-graphql-typedef": "dist/bin.js"
  },
  "engines": {
    "node": ">=18.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "npx jest"
  },
  "dependencies": {
    "@prisma/internals": "5.14.0",
    "@prisma/generator-helper": "5.14.0",
    "@prisma/client": "5.14.0",
    "prettier": "3.3.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/node": "20.14.1",
    "jest": "29.7.0",
    "prisma": "5.14.0",
    "ts-jest": "29.1.4",
    "typescript": "5.4.5",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^24.0.0"
  },
  "homepage": "https://github.com/mavvystudio/prisma-generator-graphql-typedef",
  "repository": {
    "type": "git",
    "url": "https://github.com/mavvystudio/prisma-generator-graphql-typedef"
  },
  "author": "Marc Jeric Espiritu <mavvystudio@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator",
    "graphql"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
