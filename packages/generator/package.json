{
  "name": "prisma-generator-graphql-typedef",
  "description": "Provide a description about your generator",
  "version": "1.0.1",
  "main": "dist/generator.js",
  "license": "MIT",
  "bin": {
    "prisma-generator-graphql-typedef": "dist/bin.js"
  },
  "engines": {
    "node": ">=14.0"
  },
  "scripts": {
    "start": "node dist/bin.js",
    "dev": "npx tsc -w",
    "build": "npx tsc",
    "prepack": "yarn build",
    "test": "npx jest"
  },
  "dependencies": {
    "@prisma/client": "4.13.0",
    "@prisma/generator-helper": "5.0.0",
    "@prisma/internals": "4.13.0",
    "prettier": "2.8.8"
  },
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@types/node": "20.1.1",
    "@types/prettier": "2.7.2",
    "jest": "29.5.0",
    "prisma": "4.13.0",
    "ts-jest": "29.1.0",
    "typescript": "5.0.4",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "semantic-release": "^21.0.2"
  },
  "homepage": "https://github.com/mavvy22/prisma-generator-graphql-typedef",
  "repository": {
    "type": "git",
    "url": "https://github.com/mavvy22/prisma-generator-graphql-typedef"
  },
  "author": "Marc Jeric Espiritu <mavvystudio@gmail.com>",
  "keywords": [
    "prisma",
    "prisma2",
    "generator"
  ],
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json"
          ],
          "message": "chore(release): set `package.json` to ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
